##############Practica 7 .PROPUESTA PARA CALCULAR BETAS #############


x <- c(132,129,120,113.2,105,92,84,83.2,88.4,59,80,81.5,71,69.2)
y <- c(46,48,51,52.1,54,52,59,58.7,61.6,64,61.4,54.6,58.8,58)
  
x2 <- x^2

Sxy <-((sum(x*y))-((sum(x)*sum(y))/14))
  
Sxx <- (sum(x2)-(((sum(x))^2)/14))

B1 <- Sxy/Sxx

mediax <- mean(x)
mediay <- mean(y)

B0 <-mediay-B1*mediax

B1
B0
y1<- B0+B1*115

y2<- B0+B1*138
y3<- B0+B1*55
y4<- B0+B1*99
y5<- B0+B1*290
y6<- B0+B1*71

